{
  "name": "inhouse-ai-agent-service",
  "version": "1.0.0",
  "description": "PRIMA AI Agent Service - Function-calling chatbot with Laravel integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node --env-file=.env --enable-source-maps dist/index.js",
    "build": "tsup src/index.ts --format esm,cjs --sourcemap --dts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": ["ai", "chatbot", "laravel", "function-calling", "fastify"],
  "author": "PRIMA Team",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.24.3",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/rate-limit": "^9.0.1",
    "openai": "^4.20.1",
    "zod": "^3.22.4",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "undici": "^6.19.8",
    "pino": "^9.1.0"

  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
  
